{
  "name": "Backend Container",
  "dockerComposeFile": "../../docker-compose.yml",
  "service": "backend",
  "workspaceFolder": "/workspace",
  "customizations": {
    "vscode": {
      "extensions": [
        "dbaeumer.vscode-eslint"
      ],
      "settings": {
        "terminal.integrated.shell.linux": "/bin/bash"
      }
    }
  },
  "shutdownAction": "none",
  "overrideCommand": false,
  "remoteUser": "dev",
  "containerUser": "dev"
}